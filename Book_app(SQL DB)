SELECT TOP 1000 [Name]
      ,[Phone Number]
      ,[Email ID]
      ,[Pet1 Name]
      ,[Pet1 Age]
      ,[Pet1 Species]
      ,[Pet1 Breed]
      ,[Pet2 Name]
      ,[Pet2 Age]
      ,[Pet2 Species]
      ,[Pet2 Breed]
      ,[Additional Pet Details]
      ,[Service]
      ,[Doctor]
      ,[Date]
  FROM [UR_Pets].[dbo].[Book_app]
